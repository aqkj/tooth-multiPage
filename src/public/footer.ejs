<!-- 尾部 -->
<footer class="l-footer fadeInUp wow" data-wow-duration="1.5s" data-wow-offset="0">
  <div class="l-container">
    <div class="l-footer__topnav clearfix">
      <a href="/sitemap.xml" class="l-footer__topnav__item" title="腾屹首页">腾屹首页</a>
      {dede:channel type="top"}
      <a href="[field:typeurl/]" class="l-footer__topnav__item" title="[field:typename/]">[field:typename/]</a>
      {/dede:channel}
      <a href="/sitemap.xml" class="l-footer__topnav__item" title="网站地图">网站地图</a>
    </div>
    <div class="l-footer__friendlink">LINK 友情链接：
      {dede:flink type='textall'}
      [field:link /]
      {/dede:flink}
      <!-- <a class="l-footer__friendlink__item" href="#">360</a> -->
    </div>
    <ul class="l-footer__nav clearfix">
      <li class="l-footer__nav__item">
        <span class="l-footer__nav__label">正畸托槽</span>
        <ul class="l-footer__nav__list">
          {dede:channel typeid="1"}
          <li class="l-footer__nav__list__item">
            <a href="[field:typelink/]">[field:typename/]</a>
          </li>
          {/dede:channel}
        </ul>
      </li>
      <li class="l-footer__nav__item">
        <span class="l-footer__nav__label">常见问题</span>
        <ul class="l-footer__nav__list">
          {dede:channel typeid="17"}
          <li class="l-footer__nav__list__item">
            <a href="[field:typelink/]">[field:typename/]</a>
          </li>
          {/dede:channel}
        </ul>
      </li>
      <li class="l-footer__nav__item">
        <span class="l-footer__nav__label">联系我们</span>
        <ul class="l-footer__nav__list">
          <li class="l-footer__nav__list__item">
            咨询热线：{dede:global.tel_one/}
          </li>
          <li class="l-footer__nav__list__item">
            技术支持：{dede:global.tel_two/}
          </li>
          <li class="l-footer__nav__list__item">
            联系地址：{dede:global.address/}
          </li>
          <li class="l-footer__nav__list__item">
            传真：{dede:global.fax/}
          </li>
        </ul>
      </li>
      <li class="l-footer__nav__item">
        <span class="l-footer__nav__label">企业邮箱</span>
        <ul class="l-footer__nav__list">
          <li class="l-footer__nav__list__item">{dede:global.email/}</li>
        </ul>
      </li>
      <li class="l-footer__nav__qrcode">
        <img src="<%=require('@/assets/qrcode.png')%>" title="扫一扫，用关注公众号" alt="扫一扫，用关注公众号" />
        <span>扫一扫，关注公众号</span>
      </li>
    </ul>
    <div class="l-footer__copyright">
      {dede:global.cfg_powerby/}
    </div>
  </div>
</footer>