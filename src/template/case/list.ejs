<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>{dede:field.title/}_{dede:global.cfg_webname/}</title>
  <meta name="keywords" content="{dede:field name='keywords'/}" />
  <meta name="description" content="{dede:field name='description' function='html2text(@me)'/}" />
</head>
<body>
  <%-include ../../public/header.ejs%>
  <%-include ../../public/nbanner.ejs%>
  <%-include ../../public/search.ejs%>
  <%-include ../../public/breadcrumb.ejs%>
  <div class="l-case">
    <div class="l-container">
      <div id="masonry" class="clearfix" class="sjs-default">
      {dede:list row="12" titlelen='200' infolen='200' }
        <a class="l-case__item" data-sjsel="flatty" href="[field:arcurl/]">
          <div class="l-case__main">
            <img class="l-case__img" src="[field:litpic/]" alt="[field:title/]">
            <div class="l-case__content">
              <h2 class="l-case__title">[field:title/]</h2>
              <p class="l-case__desc">
                [field:description/]
              </p>
            </div>
          </div>
        </a>
      {/dede:list}
      </div>
      <ul class="page-list">
        {dede:pagelist listsize=5 listitem="next,pre,end,index,info,pageno"/}
      </ul>
    </div>
  </div>
  <%-include ../../public/footer.ejs%>
  <script src="/templets/yiliao/static/jquery.min.js"></script>
  <script src="/templets/yiliao/static/masonry.min.js"></script>
  <script src="/templets/yiliao/static/imagesloaded.min.js"></script>
</html>